{
  "name": "@soybeanjs/headless",
  "type": "module",
  "version": "0.0.2-beta.12",
  "packageManager": "pnpm@10.22.0",
  "description": "SoybeanHeadless is a collection unstyled components for Vue 3. It is designed to be lightweight and easy to use.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/soybean-ui",
  "repository": {
    "url": "https://github.com/soybeanjs/soybean-ui.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/soybean-ui/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./constants": "./src/constants/index.ts",
    "./composables": "./src/composables/index.ts",
    "./shared": "./src/shared/index.ts",
    "./forms": "./src/forms/index.ts",
    "./nuxt": "./src/nuxt/index.ts",
    "./resolver": "./src/resolver/index.ts",
    "./namespaced": "./src/namespaced/index.ts",
    "./*": "./src/components/*/index.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm tsdown",
    "build:play": "vite build",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "peerDependencies": {
    "nuxt": ">=4.0.0",
    "valibot": ">=1.0.0",
    "vue": ">=3.2.0",
    "vue-router": ">=4.0.0",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.9",
    "@internationalized/date": "^3.10.0",
    "@internationalized/number": "^3.6.5",
    "@standard-schema/spec": "^1.0.0",
    "@tanstack/vue-virtual": "^3.13.12",
    "@vue/shared": "^3.5.24",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "@vueuse/router": "^14.0.0",
    "@vueuse/shared": "^14.0.0",
    "aria-hidden": "^1.2.6",
    "defu": "^6.1.4",
    "klona": "^2.0.6",
    "ohash": "^2.0.11"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.2.1",
    "tsdown": "0.16.5",
    "typescript": "5.9.3",
    "unplugin-vue": "7.0.7",
    "unplugin-vue-jsx": "0.8.1",
    "vue": "3.5.24",
    "vue-router": "4.6.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
