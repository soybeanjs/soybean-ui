{
  "name": "soybean-primitives",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0",
  "description": "soybean-primitives is a collection of primitive components for Vue 3. It is designed to be lightweight and easy to use.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/honghuangdc/soybean-primitives",
  "repository": {
    "url": "https://github.com/honghuangdc/soybean-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-primitives/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsdown --watch",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "devDependencies": {
    "@soybeanjs/cli": "1.2.1",
    "@soybeanjs/eslint-config": "1.6.0",
    "@types/cli-progress": "3.11.6",
    "@types/node": "22.15.17",
    "eslint": "9.26.0",
    "eslint-plugin-vue": "10.1.0",
    "lint-staged": "16.0.0",
    "simple-git-hooks": "2.13.0",
    "tsdown": "0.11.4",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "unplugin-vue": "6.2.0",
    "unplugin-vue-jsx": "0.6.1",
    "vue": "3.5.13",
    "vue-eslint-parser": "10.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
