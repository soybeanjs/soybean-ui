{
  "name": "soybean-primitives",
  "type": "module",
  "version": "0.0.1-alpha.2",
  "packageManager": "pnpm@10.11.0",
  "description": "soybean-primitives is a collection of primitive components for Vue 3. It is designed to be lightweight and easy to use.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/honghuangdc/soybean-primitives",
  "repository": {
    "url": "https://github.com/honghuangdc/soybean-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-primitives/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./constant": {
      "types": "./dist/constant/index.d.ts",
      "import": "./dist/constant/index.js",
      "require": "./dist/constant/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js",
      "require": "./dist/composables/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.js",
      "require": "./dist/nuxt/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.js",
      "require": "./dist/resolver/index.js"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsdown --watch",
    "lint": "eslint . --fix",
    "play": "vite",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "devDependencies": {
    "@nuxt/kit": "3.17.3",
    "@nuxt/schema": "3.17.3",
    "@soybeanjs/cli": "1.3.0",
    "@soybeanjs/eslint-config": "1.6.1",
    "@types/node": "22.15.19",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.27.0",
    "eslint-plugin-vue": "10.1.0",
    "happy-dom": "17.4.7",
    "lint-staged": "16.0.0",
    "simple-git-hooks": "2.13.0",
    "tsdown": "0.11.12",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "unplugin-vue": "6.2.0",
    "unplugin-vue-components": "28.5.0",
    "unplugin-vue-jsx": "0.6.1",
    "vite": "6.3.5",
    "vite-plugin-vue-devtools": "7.7.6",
    "vitest": "3.1.3",
    "vue": "3.5.14",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
