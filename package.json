{
  "name": "@soybeanjs/ui",
  "type": "module",
  "version": "0.0.2-beta.8",
  "packageManager": "pnpm@10.20.0",
  "description": "SoybeanUI is built on top of SoybeanHeadless, providing a collection of styled components for Vue 3.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/soybean-ui",
  "repository": {
    "url": "https://github.com/soybeanjs/soybean-ui.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/soybean-ui/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm build:headless && pnpm build:ui && pnpm build:css",
    "build:css": "unocss build",
    "build:headless": "pnpm --filter @soybeanjs/headless build",
    "build:play": "vite build",
    "build:ui": "pnpm tsdown",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "publish-pkg": "pnpm publish -r --access public",
    "release": "soy release",
    "reset-stub": "tsx scripts/stub.ts --reset",
    "stub": "tsx scripts/stub.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@iconify/vue": "^5.0.0",
    "@soybeanjs/headless": "workspace:^",
    "@soybeanjs/unocss-shadcn": "^0.0.1",
    "clsx": "^2.1.1",
    "fuse.js": "^7.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1"
  },
  "devDependencies": {
    "@soybeanjs/cli": "^1.4.1",
    "@soybeanjs/eslint-config": "^1.7.3",
    "@soybeanjs/unocss-preset": "^0.1.5",
    "@types/node": "^24.10.0",
    "@unocss/cli": "^66.5.5",
    "@vitest/coverage-v8": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "lint-staged": "^16.2.6",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.16.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unocss": "^66.5.5",
    "unocss-preset-animations": "^1.3.0",
    "unplugin-vue": "^7.0.4",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-jsx": "^0.8.1",
    "vite": "^7.2.2",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8",
    "vue": "^3.5.24",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.3"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
