{
  "name": "soybean-primitives",
  "type": "module",
  "version": "0.0.1-alpha.4",
  "packageManager": "pnpm@10.11.0",
  "description": "soybean-primitives is a collection of primitive components for Vue 3. It is designed to be lightweight and easy to use.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/honghuangdc/soybean-primitives",
  "repository": {
    "url": "https://github.com/honghuangdc/soybean-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-primitives/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./constant": {
      "types": "./dist/constant/index.d.ts",
      "import": "./dist/constant/index.js",
      "require": "./dist/constant/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js",
      "require": "./dist/composables/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.js",
      "require": "./dist/nuxt/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.js",
      "require": "./dist/resolver/index.js"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "build:play": "vite build",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "tsdown --watch",
    "lint": "eslint . --fix",
    "play": "vite",
    "prepare": "simple-git-hooks",
    "publish-pkg": "pnpm publish --access public",
    "release": "soy release",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "@floating-ui/vue": "1.1.6",
    "@unocss/reset": "66.1.2",
    "@vue/shared": "3.5.16",
    "aria-hidden": "1.2.6",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.511.0",
    "tailwind-merge": "3.3.0",
    "tailwind-variants": "1.0.0"
  },
  "devDependencies": {
    "@nuxt/kit": "3.17.4",
    "@nuxt/schema": "3.17.4",
    "@soybean-ui/unocss-preset": "0.0.2-beta.5",
    "@soybeanjs/cli": "1.3.0",
    "@soybeanjs/eslint-config": "1.6.1",
    "@soybeanjs/unocss-preset": "0.1.1",
    "@types/node": "22.15.28",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.27.0",
    "eslint-plugin-vue": "10.1.0",
    "happy-dom": "17.5.6",
    "lint-staged": "16.1.0",
    "simple-git-hooks": "2.13.0",
    "tsdown": "0.12.5",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "unocss": "66.1.2",
    "unocss-preset-animations": "1.2.1",
    "unplugin-vue": "6.2.0",
    "unplugin-vue-components": "28.7.0",
    "unplugin-vue-jsx": "0.6.1",
    "vite": "6.3.5",
    "vite-plugin-vue-devtools": "7.7.6",
    "vite-tsconfig-paths": "6.0.0-beta.3",
    "vitest": "3.1.4",
    "vue": "3.5.16",
    "vue-eslint-parser": "10.1.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
