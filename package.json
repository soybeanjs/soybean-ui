{
  "name": "soybean-headless",
  "type": "module",
  "version": "0.0.1-alpha.5",
  "packageManager": "pnpm@10.14.0",
  "description": "SoybeanHeadless is a collection unstyled components for Vue 3. It is designed to be lightweight and easy to use.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybean-ui/soybean-headless",
  "repository": {
    "url": "https://github.com/soybean-ui/soybean-headless.git"
  },
  "bugs": {
    "url": "https://github.com/soybean-ui/soybean-headless/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./constant": {
      "types": "./dist/constant/index.d.ts",
      "import": "./dist/constant/index.js",
      "require": "./dist/constant/index.js"
    },
    "./composables": {
      "types": "./dist/composables/index.d.ts",
      "import": "./dist/composables/index.js",
      "require": "./dist/composables/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.js",
      "require": "./dist/nuxt/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.js",
      "require": "./dist/resolver/index.js"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsdown",
    "build:nuxt": "nuxt build",
    "build:play": "vite build -c vite.play.config.ts",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "deploy:play": "wrangler deploy",
    "dev": "tsdown --watch",
    "dev:nuxt": "nuxt dev",
    "dev:play": "pnpm play",
    "generate": "nuxt generate",
    "lint": "eslint . --fix",
    "play": "vite -c vite.play.config.ts",
    "prepare": "simple-git-hooks",
    "preview": "vite preview -c vite.play.config.ts",
    "publish-pkg": "pnpm publish --access public",
    "rei": "pnpm cleanup && pnpm install",
    "release": "soy release",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy ncu"
  },
  "dependencies": {
    "@floating-ui/vue": "1.1.8",
    "@iconify/vue": "5.0.0",
    "@unocss/reset": "66.4.2",
    "@vue/shared": "3.5.18",
    "@vueuse/core": "13.6.0",
    "@vueuse/router": "13.6.0",
    "aria-hidden": "1.2.6",
    "clsx": "2.1.1",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0"
  },
  "devDependencies": {
    "@soybean-ui/unocss-preset": "0.0.2-beta.5",
    "@soybeanjs/cli": "1.3.1",
    "@soybeanjs/eslint-config": "1.7.1",
    "@soybeanjs/unocss-preset": "0.1.2",
    "@types/node": "24.2.1",
    "@unocss/nuxt": "66.4.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.32.0",
    "eslint-plugin-vue": "10.4.0",
    "happy-dom": "18.0.1",
    "lint-staged": "16.1.5",
    "nuxt": "4.0.3",
    "simple-git-hooks": "2.13.1",
    "tsdown": "0.13.4",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "unocss": "66.4.2",
    "unocss-preset-animations": "1.2.1",
    "unplugin-vue": "7.0.1",
    "unplugin-vue-components": "29.0.0",
    "unplugin-vue-jsx": "0.6.1",
    "vite": "7.1.1",
    "vite-plugin-vue-devtools": "8.0.0",
    "vite-tsconfig-paths": "6.0.0-beta.3",
    "vitest": "3.2.4",
    "vue": "3.5.18",
    "vue-eslint-parser": "10.2.0",
    "vue-router": "4.5.1",
    "vue-tsc": "3.0.5",
    "wrangler": "4.28.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
